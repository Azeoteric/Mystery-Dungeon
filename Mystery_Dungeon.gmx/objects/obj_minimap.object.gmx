            tile_add(bg_wall, 0, 0, MAP_WIDTH, MAP_HEIGHT, (xpos * MAP_WIDTH) + (x - 100), (ypos * MAP_HEIGHT) + (y - 90), -9000);
        }  
        
        if (grid[# xpos, ypos] == NODE) {
            // Draw nodes
            tile_add(bg_node, 0, 0, MAP_WIDTH, MAP_HEIGHT, (xpos * MAP_WIDTH) + (x - 100), (ypos * MAP_HEIGHT) + (y - 90), -9000);    
        }
     }
     
}

